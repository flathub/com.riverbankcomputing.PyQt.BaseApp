{
    "name": "pyqt",
    "build-options": {
        "env": {
            "EXTRA_PYQT_CONFIG_OPTS": "--disabled-feature=PyQt_OpenGL_ES2"
        }
    },
    "config-opts": [
        "--concatenate=1",
        "--confirm-license",
        "--debug",
        "--no-designer-plugin",
        "--no-make",
        "--no-qml-plugin",
        "--verbose",
        "--api-dir=/app/share/qt/qsci/api/python",
        "--qmake-setting=QMAKE_CFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--qmake-setting=QMAKE_CXXFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--scripts-dir=/app/bin",
        "--target-dir=/app/lib/python_pyver/site-packages",
        "--qt-shared"
    ],
    "make-args": [
        "--directory=build"
    ],
    "make-install-args": [
        "--directory=build"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://files.pythonhosted.org/packages/1a/54/793f2a2408fd7774361faf99ecf1e276e787e0cbc3062161e2c54d94df33/PyQt6-6.3.0.tar.gz",
            "sha256": "4fd85dcb15ea4e734b6e4e216fe9a6246779761edaf2cf7c0cce1a2303a8d31b",
            "x-checker-data": {
                "type": "pypi",
                "name": "PyQt6"
            }
        },
        {
            "type": "file",
            "path": "configure"
        },
        {
            "type": "file",
            "path": "../sip/sip-build-configure.in",
            "dest-filename": "configure.sip-build"
        }
    ],
    "modules": [
        "../sip/sip.json",
        "../pyqt-builder/pyqt-builder.json",
        "../pyqt-sip/pyqt-sip.json"
    ]
}
