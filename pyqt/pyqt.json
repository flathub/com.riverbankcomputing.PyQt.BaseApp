{
    "name": "pyqt",
    "build-options": {
        "env": {
            "EXTRA_PYQT_CONFIG_OPTS": "--disabled-feature=PyQt_OpenGL_ES2 --disabled-feature=PyQt_Permissions"
        }
    },
    "config-opts": [
        "--concatenate=1",
        "--confirm-license",
        "--debug",
        "--no-make",
        "--verbose",
        "--api-dir=/app/share/qt/qsci/api/python",
        "--qmake-setting=QMAKE_CFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--qmake-setting=QMAKE_CXXFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--scripts-dir=/app/bin",
        "--target-dir=/app/lib/python_pyver/site-packages",
        "--qt-shared"
    ],
    "make-args": [
        "--directory=build"
    ],
    "make-install-args": [
        "--directory=build"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://files.pythonhosted.org/packages/8c/2b/6fe0409501798abc780a70cab48c39599742ab5a8168e682107eaab78fca/PyQt6-6.6.1.tar.gz",
            "sha256": "9f158aa29d205142c56f0f35d07784b8df0be28378d20a97bcda8bd64ffd0379",
            "x-checker-data": {
                "type": "pypi",
                "name": "PyQt6",
                "versions": {
                    "<": "6.7"
                },
                "is-main-source": true
            }
        },
        {
            "type": "patch",
            "path": "pyqt-plugins-dir.patch"
        },
        {
            "type": "file",
            "path": "configure"
        },
        {
            "type": "file",
            "path": "../sip/sip-build-configure.in",
            "dest-filename": "configure.sip-build"
        }
    ],
    "modules": [
        "../sip/sip.json",
        "../pyqt-builder/pyqt-builder.json",
        "../pyqt-sip/pyqt-sip.json"
    ]
}
