{
    "name": "pyqt",
    "build-options": {
        "env": {
            "EXTRA_PYQT_CONFIG_OPTS": "--disabled-feature=PyQt_OpenGL_ES2 --disabled-feature=PyQt_Permissions"
        }
    },
    "config-opts": [
        "--concatenate=1",
        "--confirm-license",
        "--debug",
        "--no-make",
        "--verbose",
        "--api-dir=/app/share/qt/qsci/api/python",
        "--qmake-setting=QMAKE_CFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--qmake-setting=QMAKE_CXXFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--scripts-dir=/app/bin",
        "--target-dir=/app/lib/python_pyver/site-packages",
        "--qt-shared"
    ],
    "make-args": [
        "--directory=build"
    ],
    "make-install-args": [
        "--directory=build"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://files.pythonhosted.org/packages/18/b8/74667c8108d8481b87f8d87e6d962b64eb53ea21432cdbfbfeee4d2b4430/PyQt6-6.5.3.tar.gz",
            "sha256": "bcbbf9511b038b4924298ca10999aa36eb37a0a38d0638f895f9bba6025c0a77",
            "x-checker-data": {
                "type": "pypi",
                "name": "PyQt6",
                "versions": {
                    "<": "6.6"
                },
                "is-main-source": true
            }
        },
        {
            "type": "patch",
            "path": "pyqt-plugins-dir.patch"
        },
        {
            "type": "file",
            "path": "configure"
        },
        {
            "type": "file",
            "path": "../sip/sip-build-configure.in",
            "dest-filename": "configure.sip-build"
        }
    ],
    "modules": [
        "../sip/sip.json",
        "../pyqt-builder/pyqt-builder.json",
        "../pyqt-sip/pyqt-sip.json"
    ]
}
