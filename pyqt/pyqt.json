{
    "name": "pyqt",
    "build-options": {
        "env": {
            "EXTRA_PYQT_CONFIG_OPTS": "--disabled-feature=PyQt_OpenGL_ES2 --disabled-feature=PyQt_Permissions"
        }
    },
    "config-opts": [
        "--concatenate=1",
        "--confirm-license",
        "--debug",
        "--no-make",
        "--verbose",
        "--api-dir=/app/share/qt/qsci/api/python",
        "--qmake-setting=QMAKE_CFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--qmake-setting=QMAKE_CXXFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--scripts-dir=/app/bin",
        "--target-dir=/app/lib/python_pyver/site-packages",
        "--qt-shared"
    ],
    "make-args": [
        "--directory=build"
    ],
    "make-install-args": [
        "--directory=build"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://files.pythonhosted.org/packages/34/da/e03b7264b1e88cd553ff62a71c0c19f55690e08928130f4aae613723e535/PyQt6-6.5.2.tar.gz",
            "sha256": "1487ee7350f9ffb66d60ab4176519252c2b371762cbe8f8340fd951f63801280",
            "x-checker-data": {
                "type": "pypi",
                "name": "PyQt6",
                "versions": {
                    "<": "6.6"
                },
                "is-main-source": true
            }
        },
        {
            "type": "patch",
            "path": "pyqt-plugins-dir.patch"
        },
        {
            "type": "file",
            "path": "configure"
        },
        {
            "type": "file",
            "path": "../sip/sip-build-configure.in",
            "dest-filename": "configure.sip-build"
        }
    ],
    "modules": [
        "../sip/sip.json",
        "../pyqt-builder/pyqt-builder.json",
        "../pyqt-sip/pyqt-sip.json"
    ]
}
