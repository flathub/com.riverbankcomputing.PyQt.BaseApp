{
    "name": "pyqt-webengine",
    "build-options": {
        "env": [
            "QMAKEPATH=/app"
        ]
    },
    "config-opts": [
        "--concatenate=1",
        "--debug",
        "--verbose",
        "--qmake-setting=QMAKE_CFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--qmake-setting=QMAKE_CXXFLAGS_RELEASE='-I/usr/include/python_pyver/'",
        "--target-dir=/app/lib/python_pyver/site-packages"
    ],
    "make-args": [
        "--directory=build"
    ],
    "make-install-args": [
        "--directory=build"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://files.pythonhosted.org/packages/49/9a/69db3a2ab1ba43f762144a66f0375540e195e107a1049d7263ab48ebc9cc/PyQt6_WebEngine-6.6.0.tar.gz",
            "sha256": "d50b984c3f85e409e692b156132721522d4e8cf9b6c25e0cf927eea2dfb39487",
            "x-checker-data": {
                "type": "pypi",
                "name": "PyQt6-WebEngine",
                "versions": {"<":  "6.7"}
            }
        },
        {
            "type": "file",
            "path": "../sip/sip-build-configure.in",
            "dest-filename": "configure"
        }
    ]
}
